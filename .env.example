# Project-wide environment variables (example template)
# Copy this to `.env.local` or `.env` during setup and replace values.

# Public values exposed to the client must start with NEXT_PUBLIC_
NEXT_PUBLIC_APP_NAME=NextFSD
NEXT_PUBLIC_API_URL=https://api.example.com

# Server-side secrets (placeholder values)
NEXTAUTH_SECRET=your-production-secret-here
DATABASE_URL=postgres://user:password@localhost:5432/dbname

# Optional: next-intl default timezone
NEXT_PUBLIC_DEFAULT_TIMEZONE=UTC
